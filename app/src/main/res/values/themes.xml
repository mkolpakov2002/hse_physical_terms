        <item name="colorPrimary">@color/hse_blue</item>
        <item name="colorPrimaryVariant">@color/hse_purple</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/hse_grey</item>
        <item name="colorSecondaryVariant">@color/hse_grey_variant</item>
        <item name="colorOnSecondary">@color/white</item>
